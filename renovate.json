{
    "automerge": true,
    "enabled": true,
    "labels": ["dependency"],
    "docker": {
      "enabled": true
    },
    "github-actions": {
      "enabled": true
    },
    "packageRules": [
      {
        "automerge": true,
        "automergeStrategy": "squash",
        "matchUpdateTypes": ["digest", "minor", "patch", "pin"]
      }
    ],
    "prConcurrentLimit": 1,
    "semanticCommits": "enabled"
  }